{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev:build": "esbuild src/index.ts --platform=node --sourcemap --packages=external --bundle --target=node18 --outfile=dist/index.cjs",
    "dev:watch": "npm run dev:build -- --watch",
    "alias": "tsc-alias",
    "dev": "concurrently \"npm:alias -- --watch\" \"npm:dev:watch\"",
    "watch": "nodemon --inspect=0.0.0.0 dist/index.cjs",
    "prebuild": "rm -rf dist",
    "start": "concurrently \"npm:dev\" \"npm:watch\"",
    "volume:remove": "docker volume rm api_dev_nginx_secrets",
    "start:prod": "npm run deploy && node dist/index.js",
    "test": "npm run test:unit -- --run; npm run test:int -- --run",
    "test:unit": "vitest -c ./vitest.config.unit.ts",
    "test:int": "./scripts/run-integration.sh",
    "deploy": "prisma migrate deploy",
    "build": "NODE_ENV=production esbuild src/index.ts --platform=node --bundle --minify --target=node18 --outfile=dist/index.cjs",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint ."
  },
  "keywords": [
    "typescript",
    "express",
    "graphql"
  ],
  "author": "Chris Pruyne <chris@chrisplease.me>",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@as-integrations/aws-lambda": "^3.1.0",
    "@devoxa/prisma-relay-cursor-connection": "^3.1.0",
    "@keyv/redis": "^2.5.8",
    "@prisma/client": "^5.3.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.0.1",
    "graphql": "^16.6.0",
    "jwks-rsa": "^3.0.1",
    "keyv": "^4.5.2",
    "prisma-extension-soft-delete": "^1.0.0",
    "redis": "^4.6.6",
    "undici": "^5.27.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.16.2",
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "concurrently": "^8.0.1",
    "esbuild": "0.19.10",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "graphql-tag": "^2.12.6",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vitest": "^0.34.6",
    "vitest-mock-extended": "^1.1.3"
  }
}
