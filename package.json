    "express": "^4.18.1",
    "express-jwt": "^8.3.0",
    "graphql": "^16.6.0",
    "graphql-import-node": "^0.0.5",
    "jwks-rsa": "^3.0.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4"
  },
