{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon --inspect=0.0.0.0 dist/index.js",
    "build:dev": "concurrently \"tsc-alias --watch\" \"tsc --watch --preserveWatchOutput\"",
    "prebuild": "rimraf dist",
    "start": "NODE_ENV=develop concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "start:prod": "prisma migrate deploy && node dist/index.js",
    "test": "npm run test:unit -- --run; npm run test:int -- --run",
    "test:unit": "vitest -c ./vitest.config.unit.ts",
    "test:int": "./scripts/run-integration.sh",
    "posttest:int": "docker compose down",
    "deploy": "npx prisma migrate deploy",
    "build": "NODE_ENV=production tsc && tsc-alias",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint ."
  },
  "keywords": [
    "typescript",
    "express",
    "graphql"
  ],
  "author": "Chris Pruyne <chris@chrisplease.me>",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@devoxa/prisma-relay-cursor-connection": "^3.1.0",
    "@keyv/redis": "^2.5.8",
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^8.3.0",
    "graphql": "^16.6.0",
    "jwks-rsa": "^3.0.1",
    "keyv": "^4.5.2",
    "prisma-soft-delete-middleware": "^1.0.1",
    "redis": "^4.6.6",
    "undici": "^5.27.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.16.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "concurrently": "^8.0.1",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "graphql-tag": "^2.12.6",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vitest": "^0.34.6",
    "vitest-mock-extended": "^1.1.3"
  }
}
