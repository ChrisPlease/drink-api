{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev:build": "esbuild src/index.ts --platform=node --sourcemap --packages=external --bundle --target=node18 --outfile=dist/index.cjs",
    "dev:watch": "npm run dev:build -- --watch",
    "alias": "tsc-alias",
    "dev": "concurrently \"npm:alias -- --watch\" \"npm:dev:watch\"",
    "watch": "nodemon --inspect=0.0.0.0 dist/index.cjs",
    "prebuild": "rm -rf dist",
    "start": "concurrently \"npm:dev\" \"npm:watch\"",
    "start:prod": "npm run deploy && node dist/index.js",
    "test": "npm run test:unit -- --run; npm run test:int -- --run",
    "test:unit": "vitest -c ./vitest.config.unit.ts",
    "test:int": "./scripts/run-integration.sh",
    "deploy": "prisma migrate deploy",
    "build": "NODE_ENV=production esbuild src/index.ts --platform=node --bundle --minify --target=node18 --outfile=dist/index.cjs",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint ."
  },
  "keywords": [
    "typescript",
    "graphql"
  ],
  "author": "Chris Pruyne <chris@chrisplease.me>",
  "license": "ISC",
  "dependencies": {
    "@keyv/redis": "^2.8.3",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.4.1",
    "jwks-rsa": "^3.1.0",
    "keyv": "^4.5.4",
    "redis": "^4.6.12",
    "undici": "^5.28.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.132",
    "@types/node": "^20.11.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-istanbul": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "concurrently": "^8.2.2",
    "esbuild": "0.20.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "vitest": "^1.2.2",
    "vitest-mock-extended": "^1.3.1"
  }
}
