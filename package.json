{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon --inspect=0.0.0.0 dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "prestart": "rimraf dist",
    "start": "NODE_ENV=develop concurrently \"npm:start:dev\" \"npm:build:dev\"",
    "build": "NODE_ENV=production tsc",
    "typeorm": "typeorm-ts-node-esm",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/data-source.ts",
    "seed": "ts-node ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d ./src/database/data-source.ts"
  },
  "keywords": [
    "typescript",
    "express",
    "graphql"
  ],
  "author": "Chris Pruyne <chris@chrisplease.me>",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^8.3.0",
    "graphql": "^16.6.0",
    "jwks-rsa": "^3.0.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.2"
  },
  "devDependencies": {
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
