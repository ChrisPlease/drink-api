{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es2020"],
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "module": "commonjs",
    "sourceMap": true,
    "noEmit": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./functions/*"]
    }
  },
  "include": [
    "./functions/**/*",
  ],
  "exclude": ["./functions/graphql/prisma"]
}
