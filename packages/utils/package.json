{
  "name": "@waterlog/utils",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "esbuild src/index.ts --platform=node --minify --bundle --target=node18 --outfile=dist/index.cjs && pnpm types",
    "build:dev": "esbuild src/index.ts --platform=node --sourcemap --bundle --target=node18 --outfile=dist/index.cjs && pnpm types",
    "types": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "dev": "concurrently \"pnpm build:dev --watch\" \"pnpm types --watch\"",
    "watch": "NODE_ENV=development pnpm build:dev --watch",
    "postinstall": "pnpm build"
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "rimraf": "^5.0.5",
    "tsconfig": "workspace:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
